    "ax.text(0, 2.2, message, size = 13, horizontalalignment='left')\n",
    "\n",
    "#Calculate and add Health correlation text\n",
    "cr = mdf3[\"Health\"].corr(mdf3[\"GDP per capita\"])\n",
