   "source": [
    "mdf4[\"human capital quintile\"] = pd.qcut(mdf4[\"Human Capital\"], 5, labels = [.2, .4, .6, 0.8, 1.0] , retbins=False, precision=3, duplicates='raise')\n",
    "#split by hc index quantile\n",
    "hcqdf = mdf4.groupby(\"human capital quintile\").mean()\n",
    "hcqdf.head()"
