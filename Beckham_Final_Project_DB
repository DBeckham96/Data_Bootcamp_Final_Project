   "source": [
    "#Add column representing GDP per capita quantile to each row\n",
    "mdf3[\"GDP per capita quantile\"] = pd.qcut(mdf3[\"GDP per capita\"], 5, labels = [.2, .4, .6, 0.8, 1.0] , retbins=False, precision=3, duplicates='raise')\n",
    "#split by GDP per capita quantile\n",
    "qdf = mdf3.groupby(\"GDP per capita quantile\").mean()\n",
    "qdf.head()"
   ]
  },
