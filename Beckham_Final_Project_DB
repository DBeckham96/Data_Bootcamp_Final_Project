    "fig, ax = plt.subplots(nrows = 1, ncols = 2, figsize = (16,7))\n",
    "\n",
    "fig.text(0.5, 1, \"Wealth vs Happiness in the Richest and Poorest Countries\", horizontalalignment='center', size = 20, color = \"white\", weight = \"bold\")\n",
    "\n",
    "#plot richest countries\n",
    "\n",
